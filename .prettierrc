{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "never",
	"overrides": [],
	"singleAttributePerLine": true,
	"endOfLine": "lf",
	"quoteProps": "as-needed",
	"bracketSameLine": false,
	"htmlWhitespaceSensitivity": "css"
}
